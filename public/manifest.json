{
  "short_name": "Weather Extension",
  "name": "Weather Chrome Extension",
  "host_permissions": [
    "*://*/*"
  ],
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Weather Extension"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "version": "1.0",
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  }
}
